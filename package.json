{
  "name": "ai-seo-os",
  "version": "1.0.0",
  "description": "AI SEO Operating System - Multi-agent swarm for autonomous SEO optimization",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx scripts/seed.ts",
    "db:backup": "tsx scripts/backup.ts",
    "workers:build": "tsc -p tsconfig.workers.json",
    "workers:dev": "tsx watch workers/main-worker.ts",
    "docker:up": "docker-compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker-compose -f docker/docker-compose.yml down",
    "docker:logs": "docker-compose -f docker/docker-compose.yml logs -f",
    "k8s:apply": "kubectl apply -f k8s/",
    "k8s:delete": "kubectl delete -f k8s/",
    "vercel:deploy": "vercel --prod",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^0.0.50",
    "@ai-sdk/google": "^0.0.51",
    "@ai-sdk/openai": "^0.0.63",
    "@auth/drizzle-adapter": "^1.5.0",
    "@upstash/ratelimit": "^2.0.3",
    "@upstash/redis": "^1.34.0",
    "@vercel/kv": "^2.0.0",
    "@vercel/postgres": "^0.9.0",
    "ai": "^3.4.0",
    "bullmq": "^5.12.0",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "drizzle-orm": "^0.33.0",
    "googleapis": "^144.0.0",
    "ioredis": "^5.4.1",
    "lucide-react": "^0.441.0",
    "next": "15.0.0-canary.171",
    "next-auth": "5.0.0-beta.20",
    "pg": "^8.12.0",
    "postgres": "^3.4.4",
    "react": "19.0.0-rc-5dcb0097-20240918",
    "react-dom": "19.0.0-rc-5dcb0097-20240918",
    "recharts": "^2.12.7",
    "tailwind-merge": "^2.5.2",
    "uuid": "^10.0.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.0-rc.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.47.0",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.16.5",
    "@types/pg": "^8.11.9",
    "@types/react": "^18.3.6",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.20",
    "drizzle-kit": "^0.24.2",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.0.0-canary.171",
    "jest": "^29.7.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "tailwindcss": "^3.4.11",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
